@model IEnumerable<Ajanvarausprojekti.ViewModels.ajatListaData>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Alkuaika)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.kesto_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.opettaja_id)
        </th>
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Kesto)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Alkuaika)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.kesto_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.opettaja_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Kesto)
        </td>
    </tr>
}

</table>
